@model HashSet<string>
@{
    ViewData["Title"] = "Index";
}

<a asp-action="DeleteFirstItem">Baştan Sil</a> <!-- DeleteFirstItem parametre almadigi icin baska parametre vermedik -->

<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name" />
    <button type="submit" name="name">Kaydet</button>
</form>

@foreach (var item in Model)
{
    <h2>@item <a asp-action="DeleteItem" asp-route-name="@item">Sil</a></h2>
}